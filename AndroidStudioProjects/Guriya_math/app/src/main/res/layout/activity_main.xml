<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.vivek.guriya_math.MainActivity"
    android:background="@drawable/gaudiyamutt1"
    android:id="@+id/a">

    <ImageButton
        android:layout_width="90dp"
        android:layout_height="110dp"
        android:id="@+id/imageButton"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Bhoga Arati Afternoon"
        android:id="@+id/textView"
        android:layout_below="@+id/imageButton"
        android:layout_alignParentStart="true"
        android:layout_alignEnd="@+id/imageButton"
        android:textColor="#f41212"
        android:textSize="16dp" />

    <ImageButton
        android:layout_width="100dp"
        android:layout_height="110dp"
        android:id="@+id/imageButton2"
        android:background="@drawable/narshimadev"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <ImageButton
        android:layout_width="90dp"
        android:layout_height="110dp"
        android:id="@+id/imageButton3"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Narshimadev Arati"
        android:id="@+id/textView2"
        android:gravity="fill_horizontal"
        android:textColor="#030360"
        android:layout_alignStart="@+id/imageButton2"
        android:layout_below="@+id/imageButton2"
        android:layout_alignEnd="@+id/imageButton2"
        android:textSize="15dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Evening Arati"
        android:id="@+id/textView3"
        android:layout_below="@+id/imageButton3"
        android:layout_alignStart="@+id/imageButton3"
        android:layout_alignEnd="@+id/imageButton3"
        android:gravity="center_horizontal"
        android:textColor="#ec0909"
        android:textSize="18dp" />

    <ImageButton
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/imageButton4"
        android:background="@drawable/tulsi1"
        android:layout_below="@+id/textView"
        android:layout_alignParentStart="true"
        android:layout_marginTop="39dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Tulsi Arati"
        android:id="@+id/textView5"
        android:layout_below="@+id/imageButton4"
        android:layout_alignParentStart="true"
        android:textColor="#0c3c07"
        android:layout_alignEnd="@+id/textView" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton5"
        android:layout_alignTop="@+id/imageButton4"
        android:layout_alignStart="@+id/textView3"
        android:layout_alignBottom="@+id/imageButton4"
        android:layout_alignEnd="@+id/textView3" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Evening Temple Arati"
        android:id="@+id/textView4"
        android:textColor="#490303"
        android:layout_alignTop="@+id/textView5"
        android:layout_alignStart="@+id/imageButton5"
        android:gravity="center_horizontal" />

    <ImageButton
        android:layout_width="90dp"
        android:layout_height="110dp"
        android:id="@+id/imageButton6"
        android:layout_alignBottom="@+id/imageButton4"
        android:layout_alignStart="@+id/textView2" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Mangal Arati"
        android:id="@+id/textView6"
        android:gravity="center_horizontal"
        android:textColor="#ec0909"
        android:textSize="18dp"
        android:layout_below="@+id/imageButton6"
        android:layout_alignStart="@+id/imageButton6"
        android:layout_alignEnd="@+id/imageButton6" />

    <RelativeLayout tools:context="com.example.vivek.guriya_math.MainActivity"
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingBottom="@dimen/activity_vertical_margin"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_vertical_margin"
        android:background="@drawable/gaudiyamutt1"
        android:id="@+id/relativeLayout"
        android:layout_alignParentTop="true"
        android:layout_alignEnd="@+id/imageButton3">

        <ImageButton
            android:layout_width="90dp"
            android:layout_height="110dp"
            android:id="@+id/imageButton7"
            android:layout_alignParentStart="true" />

        <ImageButton
            android:layout_width="100dp"
            android:layout_height="110dp"
            android:id="@+id/imageButton8"
            android:background="@drawable/narshimadev"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true" />

        <ImageButton
            android:layout_width="90dp"
            android:layout_height="110dp"
            android:id="@+id/imageButton9"
            android:background="@drawable/evening"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true" />

        <ImageButton
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:id="@+id/imageButton10"
            android:background="@drawable/tulsi1"
            android:layout_below="@+id/textView7"
            android:layout_alignParentStart="true"
            android:layout_marginTop="39dp" />

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageButton11"
            android:layout_alignTop="@+id/imageButton10"
            android:layout_alignStart="@+id/textView9"
            android:layout_alignBottom="@+id/imageButton10"
            android:layout_alignEnd="@+id/textView9" />

        <ImageButton
            android:layout_width="90dp"
            android:layout_height="110dp"
            android:id="@+id/imageButton12"
            android:layout_alignBottom="@+id/imageButton10"
            android:layout_alignStart="@+id/textView8"
            android:background="@drawable/download" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="Bhoga Arati Afternoon"
            android:id="@+id/textView7"
            android:layout_below="@+id/imageButton7"
            android:layout_alignParentStart="true"
            android:layout_alignEnd="@+id/imageButton7"
            android:textColor="#f41212"
            android:textSize="16dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="Narshimadev Arati"
            android:id="@+id/textView8"
            android:gravity="fill_horizontal"
            android:textColor="#030360"
            android:layout_alignStart="@+id/imageButton8"
            android:layout_below="@+id/imageButton8"
            android:layout_alignEnd="@+id/imageButton8"
            android:textSize="15dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="Evening Arati"
            android:id="@+id/textView9"
            android:layout_below="@+id/imageButton9"
            android:layout_alignStart="@+id/imageButton9"
            android:layout_alignEnd="@+id/imageButton9"
            android:gravity="center_horizontal"
            android:textColor="#ec0909"
            android:textSize="18dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Tulsi Arati"
            android:id="@+id/textView10"
            android:layout_below="@+id/imageButton10"
            android:layout_alignParentStart="true"
            android:textColor="#0c3c07"
            android:layout_alignEnd="@+id/textView7" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Evening Temple Arati"
            android:id="@+id/textView11"
            android:textColor="#490303"
            android:layout_alignTop="@+id/textView10"
            android:layout_alignStart="@+id/imageButton11"
            android:gravity="center_horizontal" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="Mangal Arati"
            android:id="@+id/textView12"
            android:gravity="center_horizontal"
            android:textColor="#ec0909"
            android:textSize="18dp"
            android:layout_below="@+id/imageButton12"
            android:layout_alignStart="@+id/imageButton12"
            android:layout_alignEnd="@+id/imageButton12" />

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="100dp"
            android:id="@+id/imageButton13"
            android:layout_below="@+id/textView11"
            android:layout_alignParentStart="true"
            android:layout_alignEnd="@+id/textView10" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Morning Mandir Parikrama "
            android:id="@+id/textView13"
            android:layout_below="@+id/imageButton13"
            android:layout_alignParentStart="true"
            android:textColor="#3e3d02"
            android:layout_toStartOf="@+id/textView12"
            android:layout_alignEnd="@+id/textView12" />

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="100dp"
            android:id="@+id/imageButton14"
            android:layout_below="@+id/textView11"
            android:layout_alignStart="@+id/textView11"
            android:layout_alignParentEnd="true"
            android:background="@drawable/bhoge_arati" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="Bhoga Arati"
            android:id="@+id/textView14"
            android:gravity="center_horizontal"
            android:textColor="#ec0909"
            android:textSize="18dp"
            android:layout_below="@+id/imageButton14"
            android:layout_alignStart="@+id/imageButton14" />
    </RelativeLayout>

</RelativeLayout>
